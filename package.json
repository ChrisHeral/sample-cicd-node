{
  "name": "sample-cicd-node",
  "version": "1.0.0",
  "description": "Sample CICD Node project",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch && nodemon dist",
    "type-check": "npx tsc",
    "lint": "npm run type-check && npx tsc-strict && ./node_modules/.bin/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "tsc && jest",
    "test:ci": "tsc && jest --ci --reporters=default --reporters=jest-junit",
    "start": "npm run build && node ./dist/src/index.js",
    "watch": "npm run build && nodemon ./dist/src/index.js"
  },
  "author": "Christophe Heral",
  "license": "ISC",
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.0.5",
    "jest-junit": "^16.0.0",
    "jest-mock-req-res": "^1.0.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "typescript-strict-plugin": "^2.4.4"
  },
  "volta": {
    "node": "20.12.2"
  }
}
